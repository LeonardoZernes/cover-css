{
  "name": "cover-css",
  "version": "1.0.0",
  "description": "Pruebas creando proyectos",
  "main": "index.js",
  "scripts": {
    "dev": "npx parcel src/index.html --open",
    "rev-css": "npx stylelint src/css/styles.css",
    "fix-css": "npx stylelint src/css/styles.css --fix",
    "build": "rm -rf dist && parcel build src/index.html --no-source-maps --public-url ./",
    "deploy": "gh-pages -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Adrian",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.0",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  }
}
